@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

<_schema>
    a reg:RegisterItem ;
    reg:definition [ reg:entity <schema> ] ;
    reg:itemClass reg:Register ;
    reg:notation "schema" ;
    reg:register <http://test.wmocodes.info/bufr4> ;
    reg:status reg:statusExperimental ;
    reg:submitter
        [ foaf:name "Jeremy Tandy" ;
          foaf:openid <https://profiles.google.com/101323394574484397758>
        ] ;
    .
<schema>
    a reg:Register , ldp:Container ;
    rdfs:label "WMO No. 306 Vol I.2 FM 94 BUFR code-table schemata" ;
    dct:description "Schemata required to support WMO No. 306 Vol I.2 FM 94 BUFR (edition 4) code-table definitions; including definitions of structure and domain-specific metadata required to describe terms from WMO No. 306 Vol I.2 FM 94 BUFR (edition 4) code-table." ;
    reg:notation "schema" ;
    .