    reg:status reg:statusStable ;
    reg:submitter
        [ foaf:name "Jeremy Tandy" ;
          foaf:openid <https://profiles.google.com/101323394574484397758>
        ] ;
    .
<codeflag>
    a reg:Register , ldp:Container ;
    rdfs:label "GRIB Code- and Flag-tables" ;
    dct:description "WMO No. 306 Vol I.2 FM 92 GRIB (edition 2) Code- and Flag-tables." ;
    reg:notation "codeflag" ;
    .