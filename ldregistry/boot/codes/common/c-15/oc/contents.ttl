@prefix qudt-quantity:  <http://qudt.org/vocab/quantity#> .
@prefix common-core:  <http://test.wmocodes.info/common/schema/core#> .
@prefix qudt:    <http://qudt.org/schema/qudt#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix qudt-unit:  <http://qudt.org/vocab/unit#> .
@prefix common-units:  <http://test.wmocodes.info/common/schema/units#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .

<_seaSurfaceTemperature>
    a reg:RegisterItem ;
    common-core:Status common-core:statusValidation ;
    reg:definition [ reg:entity <seaSurfaceTemperature> ] ;
    reg:itemClass qudt:QuantityKind , qudt:ThermodynamicsQuantityKind ;
    reg:notation "seaSurfaceTemperature" ;
    reg:register <http://test.wmocodes.info/common/c-15/oc> ;
    reg:status reg:statusExperimental ;
    reg:submitter
        [ foaf:name "Jeremy Tandy" ;
          foaf:openid <https://profiles.google.com/101323394574484397758>
        ] ;
    .
<seaSurfaceTemperature>
    a qudt:QuantityKind , qudt:ThermodynamicsQuantityKind ;
    rdfs:label "Sea surface temperature"@en ;
    dct:description "The temperature of the surface of a water body; generally meant to be representative of the upper few meters of the water body as opposed to the skin temperature, which is the temperature of the upper few centimetres."@en ;
    qudt:generalization qudt-quantity:ThermodynamicTemperature ;
    .
<_seaState>
    a reg:RegisterItem ;
    common-core:Status common-core:statusValidation ;
    reg:definition [ reg:entity <seaState> ] ;
    reg:itemClass owl:Class ;
    reg:notation "seaState" ;
    reg:register <http://test.wmocodes.info/common/c-15/oc> ;
    reg:status reg:statusExperimental ;
    reg:submitter
        [ foaf:name "Jeremy Tandy" ;
          foaf:openid <https://profiles.google.com/101323394574484397758>
        ] ;
    .
<seaState>
    a owl:Class ;
    rdfs:label "Sea state"@en ;
    rdfs:subClassOf skos:Concept ;
    dct:description "Categorical assessment of sea surface state (or other large open body of water) based on height of waves."@en ;
    .
