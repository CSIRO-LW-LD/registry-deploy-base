@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix version:  <http://purl.org/linked-data/version#> .
@prefix ldp:  <http://www.w3.org/ns/ldp#> .
@prefix reg:  <http://purl.org/linked-data/registry#> .
@prefix ui:   <http://purl.org/linked-data/registry-ui#> .


<concept-with-mappings-and-notes> a ui:FormTemplate;
    rdfs:label "ConceptWithMappings"@en;
    ui:tooltip "Define a single skos:Concept, including mapping relations and notes"@en;
    ui:prototype [a skos:Concept];
    ui:formFields (
      [a ui:FormField; rdfs:label "id"; skos:notation "id";
            ui:tooltip "ID for concept in register (required)"@en;
            ui:property reg:notation;
            ui:required true ;
            ui:fieldType ui:labelField ]
      [a ui:FormField; rdfs:label "label"; skos:notation "label";
            ui:tooltip "Label for the concept (required)"@en;
            ui:property rdfs:label;
            ui:required true ;
            ui:fieldType ui:labelField ]
      [a ui:FormField; rdfs:label "preferred label"; skos:notation "prefLabel";
            ui:tooltip "Preferred label for the concept - only one per language"@en;
            ui:property skos:prefLabel;
            ui:fieldType ui:labelField]
      [a ui:FormField; rdfs:label "alternative label"; skos:notation "altLabel";
            ui:tooltip "Alternative label for the concept"@en;
            ui:property skos:altLabel;
            ui:fieldType ui:labelField]
      [a ui:FormField; rdfs:label "acronym, code or symbol"; skos:notation "notation";
            ui:tooltip "Code, serial number or acronym for the concept"@en;
            ui:property skos:notation;
            ui:fieldType ui:labelField]
      [a ui:FormField; rdfs:label "description or definition"; skos:notation "description";
            ui:tooltip "Description of the concept (recommended)"@en;
            ui:property dct:description;
            ui:fieldType ui:textField]
      [a ui:FormField; rdfs:label "has broader concept"; skos:notation "broader";
            ui:tooltip "Has broader concept (recommended if known)."@en;
            ui:property skos:broader;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has narrower concept"; skos:notation "narrower";
            ui:tooltip "Has narrower concept."@en;
            ui:property skos:narrower;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has related concept"; skos:notation "related";
            ui:tooltip "Has related concept."@en;
            ui:property skos:related;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has broader match"; skos:notation "broadMatch";
            ui:tooltip "Has broader match in another vocabulary."@en;
            ui:property skos:broadMatch;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has narrower match"; skos:notation "narrowMatch";
            ui:tooltip "Has narrower match in another vocabulary."@en;
            ui:property skos:narrowMatch;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has close match"; skos:notation "closeMatch";
            ui:tooltip "Has close match in another vocabulary."@en;
            ui:property skos:broadMatch;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has exact match"; skos:notation "exactMatch";
            ui:tooltip "Has exact match in another vocabulary."@en;
            ui:property skos:exactMatch;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "has related match"; skos:notation "relatedMatch";
            ui:tooltip "Has related match in another vocabulary."@en;
            ui:property skos:relatedMatch;
            ui:fieldType ui:anyURIField]
      [a ui:FormField; rdfs:label "change note"; skos:notation "changeNote";
            ui:tooltip "Change note"@en;
            ui:property skos:changeNote;
            ui:fieldType ui:textField]
      [a ui:FormField; rdfs:label "editorial note"; skos:notation "editorialNote";
            ui:tooltip "Editorial note"@en;
            ui:property skos:editorialNote;
            ui:fieldType ui:textField]
      [a ui:FormField; rdfs:label "scope note"; skos:notation "scopeNote";
            ui:tooltip "Scope note"@en;
            ui:property skos:scopeNote;
            ui:fieldType ui:textField]
      [a ui:FormField; rdfs:label "history note"; skos:notation "historyNote";
            ui:tooltip "History note"@en;
            ui:property skos:historyNote;
            ui:fieldType ui:textField]
    );
    .
